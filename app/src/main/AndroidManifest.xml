<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pro.araozu.lassiete">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity
            android:name=".CarritoActivity"
            android:label="@string/title_activity_carrito"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CarritoDActivity"
            android:label="@string/title_activity_carrito_d"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".CarritoActivity2"
            android:label="@string/title_activity_carrito2"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".HistorialPedidos"
            android:label="@string/title_activity_historial_pedidos"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".MisPedidos"
            android:label="@string/title_activity_mis_pedidos"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>